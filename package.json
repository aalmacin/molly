{
  "name": "molly",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "cl:build": "ng build molly",
    "cl:build:watch": "ng build molly --watch",
    "cl:build:publish": "npm run cl:build && sass projects/molly-component-library/src/lib/styles/globals.scss dist/molly-component-library/molly.dist.css && npm publish dist/molly-component-library",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --config .prettierrc --list-different \"./projects/**/*.{ts,js,scss,html}\"",
    "format:fix": "prettier --config .prettierrc --write \"./projects/**/*.{ts,js,scss,html}\"",
    "fix": "npm run lint:fix && npm run format:fix",
    "storybook": "start-storybook -p 6009 -s ./projects/molly-component-library",
    "build-storybook": "build-storybook && mkdirp storybook-static/assets && cpx ./projects/molly-component-library/assets/* storybook-static/assets",
    "test:g-out": "ng test --json --outputFile=jest-test-results.json || true",
    "test:update-snapshot": "ng test --updateSnapshots",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "css-loader": "^2.1.1",
    "enzyme-to-json": "^3.3.5",
    "rxjs": "~6.3.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslib": "^1.9.0",
    "tslint-config-prettier": "^1.18.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.4.2",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@nrwl/builders": "^7.8.3",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-jest": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/angular": "^5.0.11",
    "@types/jest": "^24.0.12",
    "@types/mocha": "^5.2.6",
    "@types/node": "~8.9.4",
    "@types/storybook__addon-knobs": "^5.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-require-context-hook": "^1.0.0",
    "codelyzer": "^5.1.0",
    "cpx": "^1.5.0",
    "enzyme": "^3.9.0",
    "jest": "^24.7.1",
    "jest-preset-angular": "^7.1.0",
    "mkdirp": "^0.5.1",
    "ng-packagr": "^4.2.0",
    "require-context.macro": "^1.0.4",
    "sass": "^1.20.1",
    "ts-jest": "^24.0.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
